<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="example_lines_list" model="ir.ui.view">
        <field name="name">example.lines.list</field>
        <field name="model">example.lines</field>
        <field name="type">list</field>
        <field name="arch" type="xml">
            <list editable="bottom">
                <field name="id"/>
                <field name="product_id"/>
                <field name="quantity"/>
                <field name="price"/>
                <field name="subtotal"/>
                <field name="example_id"/>
            </list>
        </field>
    </record>

    <record id="action_example_lines" model="ir.actions.act_window">
        <field name="name">Example Lines</field>
        <field name="res_model">example.lines</field>
        <field name="view_mode">list</field>
    </record>
</odoo>